    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.3",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint-parser": "^22.0.0",
    "typechain": "^8.3.2",
    "@typechain/ethers-v6": "^0.5.1"
